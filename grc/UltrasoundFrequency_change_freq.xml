<?xml version="1.0"?>
<block>
  <name>change_freq</name>
  <key>UltrasoundFrequency_change_freq</key>
  <category>[UltrasoundFrequency]</category>
  <import>import UltrasoundFrequency</import>
  <make>UltrasoundFrequency.change_freq($change_time, $start, $current, $end, $step, $reverse)</make>
  <param>
    <name>Time Per Change (s)</name>
    <key>change_time</key>
    <type>float</type>
  </param>
  <param>
    <name>Start Frequency (Hz)</name>
    <key>start</key>
    <type>float</type>
  </param>
  <param>
    <name>Default Frequency (Hz)</name>
    <key>current</key>
    <type>float</type>
  </param>
  <param>
    <name>End Frequency (Hz)</name>
    <key>end</key>
    <type>float</type>
  </param>
  <param>
    <name>Step Size (Hz)</name>
    <key>step</key>
    <type>float</type>
  </param>
  <param>
    <name>Order</name>
    <key>reverse</key>
    <value>False</value>
    <type>enum</type>
    <option>
      <name>Non-Reversing</name>
      <key>False</key>
    </option>
    <option>
      <name>Reversing</name>
      <key>True</key>
    </option>
  </param>
  <source>
    <name>center_freq</name>
    <type>message</type>
  </source>
</block>
